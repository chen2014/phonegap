<?xml version='1.0' encoding='utf-8'?>
<widget id="com.rjfun.demo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RentalCarFree</name>
    <description>
        レンタカー予約用アプリ.
    </description>
    <author email="maruko.chiba.2017@gmail.com" href="">
        千葉マルコ
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Orientation" value="portrait" />
    <plugin name="admob-demo-app-ionic" spec="^1.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
